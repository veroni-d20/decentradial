    "jest-styled-components": "7.1.1",
    "lint-staged": "13.0.3",
    "nft.storage": "7.0.0",
    "node-plop": "0.26.3",
    "omit-deep": "0.3.0",
    "plop": "2.7.6",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.6",
    "react-is": "18.2.0",
    "react-redux": "7.2.8",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "18.2.0",
